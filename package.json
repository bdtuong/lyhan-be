{
  "name": "scout-codesharing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "23.4.0"
  },
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "prepare": "husky",
    "format": "prettier .  --write",
    "clean": "rd /s /q build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && cross-env BUILD_MODE=production node ./build/src/server.js",
    "start": "cross-env BUILD_MODE=dev nodemon --experimental-modules --es-module-specifier-resolution=node server.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@tanstack/react-query": "^4.36.1",
    "async-exit-hook": "^2.0.1",
    "body-parser": "^1.20.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "highlight.js": "^11.10.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "mongodb": "^5.9.2",
    "node-modules": "^0.0.1",
    "nodemon": "^3.1.9",
    "rm": "^0.1.8",
    "start": "^5.1.0",
    "vite": "^6.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.15.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.0.1",
    "prettier": "^3.4.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waanney/scout_backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/waanney/scout_backend/issues"
  },
  "homepage": "https://github.com/waanney/scout_backend#readme",
  "description": ""
}
