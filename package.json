{
  "name": "scout-codesharing",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "engines": {
    "node": "23.4.0"
  },
  "scripts": {
    "lint": "eslint .",
    "prepare": "husky",
    "format": "prettier .  --write",
    "clean": "rimraf build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "start": "cross-env BUILD_MODE=production node ./build/src/server.js",
    "production": "cross-env BUILD_MODE=production node ./build/src/server.js",
    "dev": "cross-env BUILD_MODE=dev nodemon --exec babel-node ./src/server.js"

  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@tanstack/react-query": "^4.36.1",
    "@types/bcryptjs": "^2.4.6",
    "async-exit-hook": "^2.0.1",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cloudinary": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "dns": "^0.2.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb": "^5.9.2",
    "multer": "^1.4.5-lts.1",
    "node-modules": "^0.0.1",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.1.9",
    "rm": "^0.1.8",
    "socket.io": "^4.8.1",
    "start": "^5.1.0",
    "vite": "^6.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.15.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.0.1",
    "prettier": "^3.4.1",
    "rimraf": "^6.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "_moduleAliases": {
    "~": "src"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waanney/scout_backend.git"
  },
  "keywords": [],
  "author": "",
